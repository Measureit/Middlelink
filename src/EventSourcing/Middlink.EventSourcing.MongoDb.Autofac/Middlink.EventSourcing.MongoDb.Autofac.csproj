<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Middlink.EventSourcing.MongoDb.Autofac</PackageId>
    <Version>1.0.30.0</Version>
    <Authors>Measureit</Authors>
    <Owners>Measureit</Owners>
    <ProjectUrl>http://measureit.pl</ProjectUrl>
    <Copyright>Copyright (c) Measureit</Copyright>
    <Product>Middlink.EventSourcing.MongoDb.Autofac</Product>
    <Description>Autofac extensions for event sourcing for Middlink with store in MongoDb.</Description>
    <PackageTags>Middlink.EventSourcing.MongoDb.Autofac</PackageTags>
    <LangVersion>9.0</LangVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryType>Git</RepositoryType>
    <PackageReleaseNotes>Release notes:</PackageReleaseNotes>
    <RepositoryUrl>https://github.com/Measureit/Middlink</RepositoryUrl>
  </PropertyGroup>
  <PropertyGroup>
    <OutputPath>..\..\..\bin\AnyCPU\$(Configuration)</OutputPath>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Autofac" Version="6.0.0" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Middlink.EventSourcing.MongoDb\Middlink.EventSourcing.MongoDb.csproj" />
    <ProjectReference Include="..\Middlink.EventSourcing\Middlink.EventSourcing.csproj" />
  </ItemGroup>
</Project>